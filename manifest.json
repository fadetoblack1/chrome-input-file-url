{
  "name": "File input url",
  "version": "0.1",
  "description": "Extension that allows insert file to input using url",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"],
      "document_end": "document_end"
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
}
